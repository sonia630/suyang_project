<%@ page contentType="text/html;charset=UTF-8" %><%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %><%@ taglib prefix="fmt" uri="http://java.sun.com/jstl/fmt_rt" %><!doctype html><html><head>    <jsp:include page="../include/header.jsp"/>    <style type="text/css">        .list-header ul li{            width: 25%;        }    </style></head><body><!-- Topbar --><div class="topbar">    <ul class="am-g">        <li  class="am-u-sm-3 country"><span class="cityname">北京</span><i class="am-icon-angle-down"></i></li>        <li class="title am-text-center am-u-sm-6">太阳树小儿推拿</li>        <li class="am-u-sm-3 am-text-right"></li>    </ul></div><div class="main-containter">    <div class="content">        <!-- Slider -->        <div id="slider" class="am-slider am-slider-default"             data-am-flexslider="{playAfterPaused: 1000}">            <ul class="am-slides">                <li><a href="#"><img src="/static/images/ad.png" width="100%" height="120px"></a>                </li>                <li><a href="#"><img src="/static/images/ad2.png" width="100%" height="120px"></a>                </li>                <li><a href="#"><img src="/static/images/ad.png" width="100%" height="120px"></a>                </li>                <li><a href="#"><img src="/static/images/ad2.png" width="100%" height="120px"></a>                </li>            </ul>        </div>        <!-- Global search        <div class="seach">            <div class="am-form-icon">                <i class="am-icon-search"></i>                <input type="text" class="am-form-field border-gray" placeholder="">            </div>            <button class="am-btn am-btn-success" style="background-color:#90cb83; border:none;" >确定</button>        </div>        -->        <!-- Special column -->        <div class="am-g special-column">            <div class="am-u-sm-3 ">                <i style="background-color:#a5d47c" class="am-icon-search"></i>                <span>养生专栏</span>            </div>            <div class="am-u-sm-3">                <i style="background-color:#F4AB63 " class="am-icon-star"></i>                <span>活动专区</span></div>            <div class="am-u-sm-3">                <i style="background-color:#83c7a1 " class="am-icon-street-view"></i>                <span>名医堂</span>            </div>            <div class="am-u-sm-3">                <i style="background-color:#eb9a81 " class="am-icon-phone"></i>                <span>预约技师</span>            </div>        </div>        <!--List search-->        <div class="list-search">            <div class="am-fl">为您推荐</div>            <div class="am-fr">搜索<i class="am-icon-search"></i></div>        </div>        <!--List header-->        <div class="list-header">            <ul>                <li class="active"><span>全部项目</span></li>                <c:forEach var="cat" items="${cats}">                    <li id="${cat.catId}"><span>${cat.catName}</span></li>                </c:forEach>            </ul>        </div>        <!-- List -->        <div class="service-list clear">            <!-- ul -->            <ul class="am-list">                <c:forEach items="${service}" var="service">                    <li class="am-g">                        <div class="am-u-sm-3">                            <image src="/tools/download?name=${service.serviceInfo.pic}"                                   width="80px" height="80px"></image>                        </div>                        <div class="am-u-sm-9">                            <div class="list-content">                                <div class="am-fl">                                    <h3><a href="/menu/detail?serviceId=${service.serviceInfo.serviceId}">${service.serviceInfo.serviceName}</a></h3>                                    <p class="cost"><span><fmt:formatNumber                                            value="${service.srvProviderSrvRel.price}"                                            pattern="0.00"/>元</span><span>/${service.srvProviderSrvRel.estimatedTime}分钟</span>                                    </p>                                </div>                                <div class="am-fr">                                    <button class="am-btn am-btn-primary am-btn-xs am-radius" id="order" url="/menu/detail?serviceId=${service.serviceInfo.serviceId}">预约</button>                                </div>                                <div class="info">${service.serviceInfo.serviceSummary}</div>                            </div>                        </div>                    </li>                </c:forEach>            </ul>        </div>    </div></div><!-- Navbar --><div id="completeModal" class="am-modal am-modal-no-btn" tabindex="-1" style="display: none">    <div class="am-modal-dialog">        <div class="am-modal-bd complete-modal">            <h3 class="am-text-lg am-text-primary">欢迎来到太阳树调理之家！</h3>            <p class="am-text-left am-text-xs">您提供服务前，请您补全信息并耐心等待审核！</p>            <button class="am-btn am-btn-block am-btn-primary am-btn-lg am-vertical-align-middle " id="complete">补全信息以获取更好的服务</button>            <button class="am-btn am-btn-block am-btn-white am-btn-lg am-vertical-align-middle " id="cancel">暂时拒绝</button>        </div>    </div></div><jsp:include page="../include/customer_footer.jsp"/><jsp:include page="../include/js.jsp"/><!-- Initialize Swiper --><script>    $(function () {        // slider()        Template.init("#home");        $("#order").click(function () {            var url = $(this).attr("url");            window.location.href = url;        });        $.getJSON("/home/complete", {}, function (need) {            if (need) {                var $modal = $('#completeModal')                $modal.modal();                $("#complete").click(function () {                    window.location.href="/user";                })                $("#cancel").click(function () {                    $modal.modal('close');                })            }        });    });    /*      var slider = function () {        $('#slider').flexslider({          playAfterPaused: 8000,          before: function(slider) {            if (slider._pausedTimer) {              window.clearTimeout(slider._pausedTimer);              slider._pausedTimer = null;            }          },          after: function(slider) {            var pauseTime = slider.vars.playAfterPaused;            if (pauseTime && !isNaN(pauseTime) && !slider.playing) {              if (!slider.manualPause && !slider.manualPlay && !slider.stopped) {                slider._pausedTimer = window.setTimeout(function() {                  slider.play();                }, pauseTime);              }            }          }          // 设置其他参数        });      }*/</script></body></html>